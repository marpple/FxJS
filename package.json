{
  "name": "fxjs",
  "version": "0.16.3",
  "description": "Functional Extensions for modern Javascript",
  "main": "index.cjs",
  "exports": {
    "import": "./index.js",
    "require": "./index.cjs"
  },
  "type": "module",
  "sideEffects": false,
  "scripts": {
    "transpile:internal": "BABEL_ENV=cjs babel .internal --out-dir .internal --copy-files --out-file-extension .cjs",
    "transpile:strict": "BABEL_ENV=cjs babel Strict --out-dir Strict --copy-files --out-file-extension .cjs",
    "transpile:lazy": "BABEL_ENV=cjs babel Lazy --out-dir Lazy --copy-files --out-file-extension .cjs",
    "transpile:concurrency": "BABEL_ENV=cjs babel Concurrency --out-dir Concurrency --copy-files --out-file-extension .cjs",
    "transpile:index": "BABEL_ENV=cjs babel index.js --out-file index.cjs --copy-files ",
    "transpile": "npm run transpile:internal && npm run transpile:strict && npm run transpile:lazy && npm run transpile:concurrency && npm run transpile:index",
    "generate:index": "node index_generator.js",
    "build": "npm run generate:index && npm run transpile && npm run bundle",
    "bundle:modern": "NODE_ENV=production BABEL_ENV=modern webpack --config webpack.config.cjs",
    "bundle:legacy": "NODE_ENV=production BABEL_ENV=legacy webpack --config webpack.config.cjs",
    "bundle": "npm run bundle:modern && npm run bundle:legacy",
    "test": "mocha 'test/spec.js' --timeout 10000"
  },
  "files": [
    ".internal",
    "Concurrency",
    "Lazy",
    "Strict",
    "dist",
    "index.js"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marpple/FxJS.git"
  },
  "author": "Indong Yoo <idy@marpple.com>",
  "contributors": [
    {
      "name": "Indong Yoo",
      "email": "idy@marpple.com"
    },
    {
      "name": "Piljung Park",
      "email": "pjp@marpple.com"
    },
    {
      "name": "Dohyeong Lee",
      "email": "dhl@marpple.com"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/marpple/FxJS/issues"
  },
  "homepage": "https://github.com/marpple/FxJS#readme",
  "dependencies": {
    "core-js": "^3.6.5",
    "regenerator-runtime": "^0.13.7"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "babel-loader": "^8.1.0",
    "chai": "^4.2.0",
    "mocha": "^8.2.0",
    "prettier": "^2.1.2",
    "terser-webpack-plugin": "^5.0.3",
    "webpack": "^5.3.1",
    "webpack-cli": "^4.1.0"
  },
  "directories": {
    "test": "test"
  },
  "keywords": [
    "functional",
    "fp",
    "fxjs"
  ],
  "unpkg": "dist/",
  "jsdelivr": "dist/",
  "np": {
    "yarn": false
  }
}
